doctype html
html(lang = "en")
  head
    title FilmHaven
    meta(name = "description", content = "A catalog of thousands of movies & shows.")
    meta(charset = "UTF-8")
    meta(name = "viewport", content = "user-scalable=no, initial-scale=1, maximum-scale=1, minimum-scale=1, width=device-width, height=device-height")
    meta(name = "theme-color", media = "(prefers-color-scheme: dark)", content = "#0F0F0F")
    meta(name = "theme-color", media = "(prefers-color-scheme: light)", content = "#ddd8d8")
    link(rel = "apple-touch-icon", sizes = "192x192", href = "data:image/png;base64,")
    link(rel = "icon", type = "image/png", sizes = "32x32", href = "data:image/png;base64,")
    link(rel = "manifest", href = `data:application/json;base64,${Buffer.from(JSON.stringify({
      "name": "FilmHaven",
      "short_name": "FilmHaven",
      "icons": [
        {
          "src": "data:image/png;base64,",
          "type": "image/png",
          "sizes": "192x192"
        },
        {
          "src": "data:image/png;base64,",
          "type": "image/png",
          "sizes": "512x512"
        },
        {
          "src": "data:image/png;base64,",
          "type": "image/png",
          "sizes": "192x192",
          "purpose": "any maskable"
        },
        {
          "src": "data:image/png;base64,",
          "type": "image/png",
          "sizes": "512x512",
          "purpose": "any maskable"
        }
      ],
      "background_color": "#141414",
      "theme_color": "#0F0F0F",
      "display": "standalone",
      "start_url": "https://film-haven.vercel.app"
    })).toString("base64")}`)
    each cssFile in htmlWebpackPlugin.files.css
      style !{compilation.assets[cssFile.substr(htmlWebpackPlugin.files.publicPath.length)].source()}
  body
    script(src="https://cdn.jsdelivr.net/npm/venom-player@0.2.88")
    each jsFile in htmlWebpackPlugin.files.js
      script !{compilation.assets[jsFile.substr(htmlWebpackPlugin.files.publicPath.length)].source()}
